{
  "overwrite": true,
  "schema": "../core/schema.graphql",
  "documents": "src/**/*.graphql",
  "generates": {
    "src/app/services/types.generated.ts": {
      "plugins": [
        "typescript"
      ],
      "config": {
        "maybeValue": "T"
      }
    },
    "src/": {
      "preset": "near-operation-file",
      "presetConfig": {
        "baseTypesPath": "app/services/types.generated.ts"
      },
      "plugins": [
        {
          "add": {
            "content": "/* eslint-disable */ /**\n *\n * THIS FILE IS AUTOGENERATED, DO NOT EDIT IT!\n *\n * instead, edit one of the `.graphql` files in this project and run\n *\n * npm run graphql-codegen\n *\n * for this file to be re-created\n */\n"
          }
        },
        {
          "add": {
            "content": "module.hot?.accept();"
          }
        },
        "typescript-operations",
        {
          "typescript-rtk-query": {
            "importBaseApiFrom": "app/services/baseApi",
            "exportHooks": true,
            "overrideExisting": "module.hot?.status() === \"apply\""
          }
        }
      ],
      "config": {
        "maybeValue": "T"
      }
    },
    ".introspection.json": {
      "plugins": [
        "introspection"
      ]
    }
  }
}