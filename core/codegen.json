{
  "overwrite": true,
  "schema": "schema.graphql",
  "generates": {
    "./schema_types.d.ts": {
      "plugins": [
        "typescript"
      ],
      "config": {
        "maybeValue": "T"
      }
    },
    "./graphql.schema.json": {
      "plugins": [
        "introspection"
      ]
    }
  }
}